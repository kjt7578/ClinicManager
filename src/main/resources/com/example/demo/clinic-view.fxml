<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="405.0" prefWidth="741.0" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.ClinicManagerController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <TabPane prefHeight="563.0" prefWidth="417.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
            <Tab fx:id="office_appointment" text="Office Appointments">
                <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="316.0" prefWidth="551.0">
                        <children>
                            <TextField fx:id="office_patient_first_name" layoutX="17.0" layoutY="96.0" promptText="Enter Patient First Name" />
                            <DatePicker fx:id="office_date_of_birth" layoutX="17.0" layoutY="166.0" prefHeight="22.0" prefWidth="159.0" promptText="Date Of Birth" />
                            <TextArea fx:id="status_messages" editable="false" layoutX="205.0" layoutY="22.0" prefHeight="258.0" prefWidth="480.0" promptText="Status message" />
                            <Button fx:id="office_schedule_button" layoutX="24.0" layoutY="259.0" mnemonicParsing="false" onAction="#handleScheduleOffice" text="Schedule" />
                            <TextField fx:id="office_patient_last_name" layoutX="17.0" layoutY="129.0" promptText="Enter Patient Last Name" />
                            <Button fx:id="office_cancel_button" layoutX="112.0" layoutY="259.0" mnemonicParsing="false" text="Cancel" />
                            <DatePicker fx:id="office_appointment_date" layoutX="17.0" layoutY="22.0" prefHeight="22.0" prefWidth="159.0" promptText="Appointment Date" />
                            <ComboBox fx:id="office_timeslot_selection" layoutX="19.0" layoutY="56.0" prefHeight="26.0" prefWidth="156.0" promptText="Timeslot" />
                            <ComboBox fx:id="office_provider_selection" layoutX="19.0" layoutY="205.0" prefHeight="26.0" prefWidth="156.0" promptText="Provider" />
                        </children>
                    </AnchorPane>
                </content>
            </Tab>
            <Tab fx:id="imaging_appointment" text="Imaging Appointments">
                <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="316.0" prefWidth="551.0">
                        <children>
                            <TextField fx:id="imaging_patient_first_name" layoutX="17.0" layoutY="96.0" promptText="Enter Patient First Name" />
                            <DatePicker fx:id="imaging_date_of_birth" layoutX="17.0" layoutY="166.0" prefHeight="22.0" prefWidth="159.0" promptText="Date Of Birth" />
                            <TextArea fx:id="status_messages1" editable="false" layoutX="205.0" layoutY="22.0" prefHeight="258.0" prefWidth="480.0" promptText="Status message" />
                            <Button fx:id="imaging_schedule_button" layoutX="22.0" layoutY="286.0" mnemonicParsing="false" onAction="#handleScheduleImaging" text="Schedule" />
                            <TextField fx:id="imaging_patient_last_name" layoutX="17.0" layoutY="129.0" promptText="Enter Patient Last Name" />
                            <Button fx:id="imaging_cancel_button" layoutX="112.0" layoutY="286.0" mnemonicParsing="false" text="Cancel" />
                            <DatePicker fx:id="imaging_appointment_date" layoutX="17.0" layoutY="22.0" prefHeight="22.0" prefWidth="159.0" promptText="Appointment Date" />
                            <ComboBox fx:id="imaging_timeslot_selection" layoutX="19.0" layoutY="56.0" prefHeight="26.0" prefWidth="156.0" promptText="Timeslot" />
                            <ComboBox fx:id="imaging_provider_selection" layoutX="19.0" layoutY="205.0" prefHeight="26.0" prefWidth="156.0" promptText="Provider" />
                     <ComboBox fx:id="imaging_service" layoutX="18.0" layoutY="245.0" prefHeight="26.0" prefWidth="156.0" promptText="Imaging Service" />
                        </children>
                    </AnchorPane>
                </content>
            </Tab>
            <Tab fx:id="reschedule" text="Reschedule">
                <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="329.0" prefWidth="421.0">
                        <children>
                            <TextField fx:id="re_patient_first_name" layoutX="18.0" layoutY="108.0" promptText="Enter Patient First Name" />
                            <DatePicker fx:id="re_date_of_birth" layoutX="19.0" layoutY="180.0" prefHeight="22.0" prefWidth="159.0" promptText="Date Of Birth" />
                            <TextArea fx:id="status_messages2" editable="false" layoutX="204.0" layoutY="22.0" prefHeight="260.0" prefWidth="480.0" promptText="Status message" />
                            <Button fx:id="reschedule_button1" layoutX="58.0" layoutY="260.0" mnemonicParsing="false" text="Reschedule" />
                            <TextField fx:id="re_patient_last_name" layoutX="18.0" layoutY="141.0" promptText="Enter Patient Last Name" />
                            <DatePicker fx:id="re_appointment_date" layoutX="17.0" layoutY="29.0" prefHeight="22.0" prefWidth="159.0" promptText="Appointment Date" />
                            <ComboBox fx:id="re_timeslot_selection" layoutX="18.0" layoutY="65.0" prefHeight="26.0" prefWidth="156.0" promptText="Timeslot" />
                            <ComboBox fx:id="re_newtimeslot_selection" layoutX="18.0" layoutY="215.0" prefHeight="26.0" prefWidth="156.0" promptText="New Timeslot" />
                        </children>
                    </AnchorPane>
                </content>
            </Tab>
            <Tab fx:id="display_stuff" text="Display">
                <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="535.0" prefWidth="417.0">
                        <children>
                            <ComboBox fx:id="display_selector" layoutX="27.0" layoutY="14.0" onAction="#handleDisplaySelection" prefHeight="26.0" prefWidth="514.0" promptText="Select Display Option" />
                            <TextArea fx:id="display_text_area" editable="false" layoutX="28.0" layoutY="52.0" prefHeight="257.0" prefWidth="652.0" promptText="Selected display will be displayed here" />
                        </children>
                    </AnchorPane>
                </content>
            </Tab>
        </tabs>
    </TabPane>
    <BorderPane prefHeight="21.0" prefWidth="421.0" />
</VBox>
