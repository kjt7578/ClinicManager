<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="325.0" prefWidth="461.0" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.ClinicManagerController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <TabPane prefHeight="563.0" prefWidth="417.0" tabClosingPolicy="UNAVAILABLE">
     <tabs>
       <Tab fx:id="office_appointments" text="Office Appointments">
         <content>
           <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="329.0" prefWidth="421.0">
                  <children>
                     <TextField fx:id="patient_first_name" layoutX="14.0" layoutY="63.0" promptText="Enter Patient First Name" />
                     <DatePicker fx:id="date_of_birth" layoutX="15.0" layoutY="134.0" prefHeight="22.0" prefWidth="159.0" promptText="Date Of Birth" />
                     <TextArea fx:id="status_messages" editable="false" layoutX="211.0" layoutY="22.0" prefHeight="179.0" prefWidth="184.0" promptText="Status message" />
                     <Button fx:id="schedule_button" layoutX="225.0" layoutY="210.0" mnemonicParsing="false" text="Schedule" />
                     <TextField fx:id="patient_last_name" layoutX="14.0" layoutY="100.0" promptText="Enter Patient Last Name" />
                     <Button fx:id="cencel_button11" layoutX="318.0" layoutY="210.0" mnemonicParsing="false" text="Cancel" />
                     <DatePicker fx:id="appointment_date" layoutX="15.0" layoutY="23.0" prefHeight="22.0" prefWidth="159.0" promptText="Appointment Date" />
                     <ComboBox fx:id="timeslot_selector1" layoutX="17.0" layoutY="169.0" prefHeight="26.0" prefWidth="156.0" promptText="Timeslot" />
                     <ComboBox fx:id="provider_selector11" layoutX="17.0" layoutY="208.0" prefHeight="26.0" prefWidth="156.0" promptText="Provider" />
                  </children>
               </AnchorPane>
         </content>
       </Tab>
         <Tab fx:id="imaging_appointments1" text="Imaging Appointments">
            <content>
               <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="535.0" prefWidth="417.0">
                  <children>
                     <TextField fx:id="patient_name1" layoutX="14.0" layoutY="14.0" promptText="Enter Patient Name" />
                     <DatePicker fx:id="date_of_birth1" layoutX="209.0" layoutY="14.0" promptText="Date Of Birth" />
                     <RadioButton layoutX="20.0" layoutY="52.0" mnemonicParsing="false" text="Schedule" />
                     <RadioButton layoutX="20.0" layoutY="81.0" mnemonicParsing="false" text="Reschedule" />
                     <RadioButton layoutX="20.0" layoutY="112.0" mnemonicParsing="false" text="Cancel" />
                     <TextArea fx:id="status_messages1" editable="false" layoutX="209.0" layoutY="106.0" prefHeight="156.0" prefWidth="205.0" promptText="Status message" />
                     <ComboBox fx:id="myComboBox" layoutX="19.0" layoutY="151.0" prefWidth="150.0" promptText="Select a service" />
                     <Button fx:id="submit_button" layoutX="8.0" layoutY="239.0" mnemonicParsing="false" text="Submit" />
                     <DatePicker layoutX="209.0" layoutY="52.0" promptText="Select Appointment Date" />
                  </children>
               </AnchorPane>
            </content>
         </Tab>
         <Tab fx:id="display_stuff" text="Display">
            <content>
               <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="535.0" prefWidth="417.0">
                  <children>
                     <ComboBox fx:id="display_selector" layoutX="27.0" layoutY="14.0" prefHeight="26.0" prefWidth="367.0" promptText="Select Display Option" />
                     <TextArea fx:id="display_text_area" editable="false" layoutX="28.0" layoutY="52.0" prefHeight="200.0" prefWidth="367.0" promptText="Selected display will be displayed here" />
                  </children>
               </AnchorPane>
            </content>
         </Tab>
     </tabs>
   </TabPane>
   <BorderPane prefHeight="21.0" prefWidth="421.0" />
</VBox>
